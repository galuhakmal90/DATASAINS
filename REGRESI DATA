pch = 19,
xlab = "Kunjungan dokter",
ylab = "Penjualan sereal",
main = "Kunjungan dokter dengan penjualan sereal (delay 3 bulan)",
col = "blue")
plot(___, ___,
pch = 19,
xlab = "Kunjungan dokter",
ylab = "Penjualan sereal",
main = "Kunjungan dokter dengan penjualan sereal (delay 4 bulan)",
col = "blue")
plot(___, ___,
pch = 19,
xlab = "Kunjungan dokter",
