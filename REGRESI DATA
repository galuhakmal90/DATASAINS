## Delayed period data.
## Berdasarkan scatter plot dari efek delayed period, dapat dilihat bahwa delayed period selama 4 bulan merupakan
## variabel dengan hubungan yang cukup kuat dengan kunjungan dokter.
library (dplyr)
data_delayed_effect = data.frame(
bulan = ___,
tahun = ___,
kunjungan_dokter = ___,
penjualan_sereal = ___,
penjualan_sereal_1 = ___(datagabungan$penjualan_sereal),
penjualan_sereal_2 = ___(datagabungan$penjualan_sereal,___),
penjualan_sereal_3 = ___(datagabungan$penjualan_sereal,___),
penjualan_sereal_4 = ___(datagabungan$penjualan_sereal,___),
penjualan_sereal_5 = ___(datagabungan$penjualan_sereal,___),
penjualan_sereal_6 = ___(datagabungan$penjualan_sereal,___)
)
head(data_delayed_effect)


plot(___, ___,
pch = 19,
xlab = "Kunjungan dokter",
ylab = "Penjualan sereal",
main = "Kunjungan dokter dengan penjualan sereal (delay 1 bulan)",
col = "blue")
plot(___, ___,
pch = 19,
xlab = "Kunjungan dokter",
ylab = "Penjualan sereal",
main = "Kunjungan dokter dengan penjualan sereal (delay 2 bulan)",
col = "blue")
